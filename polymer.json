{
    "entrypoint": "index.html",
    "shell": "src/my-app.html",
    "fragments": [
        "src/home.html",
        "src/products.html",
        "src/skills.html",
        "src/work.html",
        "src/contact.html",
        "src/view404.html"
    ],
    "sources": [
        "src/**/*",
        "images/**/*",
        "bower.json"
    ],
    "extraDependencies": [
        "manifest.json",
        "bower_components/webcomponentsjs/*.js"
    ],
    "lint": {
        "rules": ["polymer-2"]
    },
    "builds": [{
        "preset": "es5-bundled"
    }]
